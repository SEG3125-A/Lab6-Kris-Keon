<html lang="en-US">
<head>
    <title>Survey Analysis</title>
</head>

<body>
    <h1>Survey analysis</h1>
    <h2>First Name</h2>
    <ul>
    <% for (var i=0; i<results[0].length; i++){ %>
        <li><%= results[0][i].color %> :  <%= results[0][i].count %></li>
    <% } %>    
    </ul>
    <h2>Last Name</h2>
    <ul>
    <% for (var i=0; i<results[1].length; i++){ %>
        <li><%= results[1][i].fruit %> :  <%= results[1][i].count %></li>
    <% } %>    
    </ul>
    <h2>Drawn To First</h2>
    <ul>
    <% for (var i=0; i<results[2].length; i++){ %>
        <li><%= results[2][i].color %> :  <%= results[2][i].count %></li>
    <% } %>    
    </ul>
    <h2>Success</h2>
    <ul>
    <% for (var i=0; i<results[2].length; i++){ %>
        <li><%= results[3][i].animal %> :  <%= results[3][i].count %></li>
    <% } %>    
    </ul>
    <h2>Not Like</h2>
    <ul>
    <% for (var i=0; i<results[2].length; i++){ %>
        <li><%= results[4][i].animal %> :  <%= results[4][i].count %></li>
    <% } %>    
    </ul>
    <h2>Advice</h2>
    <ul>
    <% for (var i=0; i<results[2].length; i++){ %>
        <li><%= results[5][i].animal %> :  <%= results[5][i].count %></li>
    <% } %>    
    </ul>
</body>
</html>